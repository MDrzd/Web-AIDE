    "</select>"
  ]
},
{
  "prefix": "table",
  "description": "HTML Table",
  "body": [
    "<table>",
    "  <thead>",
    "    <tr>",
    "      <th>${1:Header 1}</th>",
    "      <th>${2:Header 2}</th>",
    "    </tr>",
    "  </thead>",
    "  <tbody>",
    "    <tr>",
    "      <td>${3:Data 1}</td>",
    "      <td>${4:Data 2}</td>",
    "    </tr>",
    "  </tbody>",
    "</table>"
  ]
},
{
  "prefix": "nav",
  "description": "HTML Navigation",
  "body": [
    "<nav>",
    "\t<ul>",
    "\t<li><a href=\"${1:url}\">${2:text}</a></li>",
    "\t</ul>",
    "</nav>"
  ]
},
{
  "prefix": "template",
  "description": "HTML Template",
  "body": [
