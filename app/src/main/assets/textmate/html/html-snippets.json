  "description": "HTML Label",
  "body": [
    "<label for=\"${1:id}\">${0}</label>"
  ]
},
{
  "prefix": "button",
  "description": "HTML Button",
  "body": [
    "<button type=\"${1:type}\">${0}</button>"
  ]
},
