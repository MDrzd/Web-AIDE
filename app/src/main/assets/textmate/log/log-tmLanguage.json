{
  "name": "LogCat",
  "version": "0.0.1",
  "scopeName": "source.log",
  "patterns": [
    {"name": "log.err", "match": "^\\[(.*)\\] [0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2} \\bE\\b (.*)"},
    {"name": "log.warn", "match": "^\\[(.*)\\] [0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2} \\bW\\b (.*)"},
    {"name": "log.info", "match": "^\\[(.*)\\] [0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2} \\bI\\b (.*)"},
    {"name": "log.debug", "match": "^\\[(.*)\\] [0-9]{1,2}:[0-9]{1,2}:[0-9]{1,2} \\bD\\b (.*)"}
  ]
}