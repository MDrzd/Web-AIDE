{
    "name": "solarized_light",
    "settings": [
        {
            "settings": {
                "background": "#FDF6E3",
                "foreground": "#657B83",
                "lineHighlight": "#EEE8D5",
                "selection": "#878b9180",
                "highlightedDelimetersForeground": "#B58900"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#93A1A1"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#2AA198"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#D33682"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "Function",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#268BD2"
            }
        },
        {
            "name": "Class",
            "scope": "entity.name.type.class",
            "settings": {
                "foreground": "#CB4B16"
            }
        },
        {
            "name": "HTML Tag",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#268BD2"
            }
        },
        {
            "name": "HTML Attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#93A1A1"
            }
        },
        {
            "name": "log.err",
            "scope": "log.err",
            "settings": {
                "foreground": "#DC322F"
            }
        },
        {
            "name": "log.info",
            "scope": "log.info",
            "settings": {
                "foreground": "#859900"
            }
        },
        {
            "name": "log.warn",
            "scope": "log.warn",
            "settings": {
                "foreground": "#B58900"
            }
        },
        {
            "name": "log.debug",
            "scope": "log.debug",
            "settings": {
                "foreground": "#B58900"
            }
        },
        {
            "name": "log.time",
            "scope": "log.time",
            "settings": {
                "foreground": "#268BD2"
            }
        }
    ]
}