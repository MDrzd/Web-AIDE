{
    "name": "quiet_dark",
    "settings": [
        {
            "settings": {
                "background": "#1E1E1E",
                "foreground": "#E0E0E0",
                "lineHighlight": "#2D2D2D",
                "selection": "#3A4A5A",
                "highlightedDelimetersForeground": "#A67FCC"
            }
        },
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6A7A89"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#8E8E8E"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#5D9B5D"
            }
        },
        {
            "name": "Numbers",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#D19A66"
            }
        },
        {
            "name": "Keywords",
            "scope": "keyword",
            "settings": {
                "foreground": "#6D8DDD"
            }
        },
        {
            "name": "Functions",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#DD6B6B"
            }
        },
        {
            "name": "Classes",
            "scope": "entity.name.type.class",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#A67FCC"
            }
        },
        {
            "name": "HTML Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#6D8DDD"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8E9BA8"
            }
        },
        {
            "name": "log.err",
            "scope": "log.err",
            "settings": {
                "foreground": "#FF6B6B"
            }
        },
        {
            "name": "log.info",
            "scope": "log.info",
            "settings": {
                "foreground": "#5D9B5D"
            }
        },
        {
            "name": "log.warn",
            "scope": "log.warn",
            "settings": {
                "foreground": "#D19A66"
            }
        }
    ]
}